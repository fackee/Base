package com.example.sort;
/**
 * 归并排序
 * */
public class Merge {
	public static void merge(Comparable a[],int low,int mid,int high){
		int i = low;
		int j = mid+1;
		Comparable aux[] = new Comparable[high];
		for(int k=low;k<=high;k++){
			aux[k] = a[k];
		}
		for(int k=low;k<=high;k++){
			
		}
	}
}
